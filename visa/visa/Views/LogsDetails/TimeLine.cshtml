@model IEnumerable<visa.Models.LogsDetails>
    @{
        ViewBag.Title = "TimeLine";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>TimeLine</h2>

    <div class="row">
        <div class="col-md-12">
            <ul class="timeline">
                @foreach (var tt in Model)
                {
                <li class="timeline-item">
                    <div class="timeline-date"><span>@tt.Date.ToLower()</span></div>
                    <div class="timeline-content">
                        <div class="timeline-avatar"><img src="assets/img/avatar6.png" alt="Avatar" class="circle"></div>
                        <div class="timeline-header">
                            <span class="timeline-time">@tt.Time</span><span class="timeline-autor">@tt.sid</span>
                            <p class="timeline-activity"><b>@tt.Title </b> /  @tt.Message</p>
                        </div>
                    </div>
                </li>
                }
               
            </ul>
        </div>
    </div>
